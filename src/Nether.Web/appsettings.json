{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  // Add these configuration values via environment variables or override on deployment
  "EventHub": {
    "KeyName": "",
    "AccessKey": "",
    "Resource": "",
    "Ttl" :  "24:00:00"
  },

  "LeaderboardStore": {
    "factory": {
      "type": "Nether.Data.MongoDB.Leaderboard.MongoDBLeaderboardStoreConfigurationFactory",
      "assembly": "Nether.Data.MongoDB"
    },
    "properties": {
      "ConnectionString": "mongodb://localhost:27017",
      "DatabaseName": "leaderboard"
    }
  },

  "AnalyticsIntegrationClient": {
    "factory": {
      "type": "Nether.Integration.Default.Analytics.AnalyticsIntegrationClientConfigurationFactory",
      "assembly": "Nether.Integration.Default"
    },
    "properties": {
      "AnalyticsBaseUrl": "http://localhost:5000/api/"
    }
  }
