<h2>Secure</h2>

<a href="/home/callapi">Call API</a>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }

    <dt>access token</dt>
    <dd>@ViewBag.AccessToken</dd>

    <dt>identity token</dt>
    <dd>@ViewBag.IdentityToken</dd>

    <dt>User.Identity.Name</dt>
    <dd>@User.Identity.Name</dd>
</dl>